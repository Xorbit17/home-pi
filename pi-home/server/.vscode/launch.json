{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: run job ART",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "job",
        "ART",
        "--params-json",
        "{\"source_image_id\":1}"
